 =  useState(true);\n  const [iconColor,setIconColor] =  useState('#303030');\n  const [scrolled,setScrolled] = useState(false);\n  const handleClick = () => setClick(!click);\n  const closeMobileMenu = () => setClick(false);\n  const showButton = () => {\n    if (window.innerWidth <= 960) {\n      setButton(false);\n    } else setButton(true);\n  }\n  const styleChecker = (style) => {\n    if (style === 'primary') {\n      setIconColor('#ebebeb');\n    } else if (style ===  'dark')  {\n      setIconColor('#ebebeb');\n    } else if (style === 'light') {\n      setIconColor('#303030');\n    }  else setIconColor('#303030');\n  }\n  const changeNavbar = () => {\n    if (window.scrollY > 5) {\n      setScrolled(true);\n    } else {\n      setScrolled(false);\n    }\n  }\n  useEffect(() =>  {\n    showButton();\n    styleChecker(style);\n  },[style]);\n  window.addEventListener('resize', showButton);\n  window.addEventListener('scroll', changeNavbar);\n  \n  return (\n    <IconContext.Provider value={{color:iconColor}}>\n      <div className={`navbar ${style ? 'navbar--'+style : ''} ${scrolled? \"active\": \"\"}`} >\n        <div className=\"navbar-container container\">\n          <Link className={`navbar-logo ${style ? 'navbar-logo--'+style : ''}`} to=\"/\">\n            <h1>{logoName}</h1>\n          </Link>\n          <div className=\"menu-icon\" onClick={handleClick}>\n            {click ? <FaTimes/>  : <FaBars/>}\n          </div>\n          <ul className={click ? `nav-menu ${style ? 'nav-menu--'+style  : '' } active` : \"nav-menu\"}>\n            {navData.map((link) => (\n              <li className=\"nav-item\">\n                  <Link className={`nav-links ${style ? 'nav-links--'+style : ''}`} to={link.to} onClick={closeMobileMenu} key={link.name}>{link.name}</Link>\n              </li>)\n            )}\n          </ul>\n        </div>\n      </div>\n    </IconContext.Provider>\n  )\n}\n","/Users/baileyritchie/Desktop/Projects/portfolio-2021/src/Components/Writing/Writing.js",["85"],"// component for writing page\nimport React from 'react';\nimport './Writing.css';\nimport {gql} from 'apollo-boost';\nimport {useQuery} from '@apollo/react-hooks';\nimport Loading from '../Loading/Loading';\n\nconst getPosts = gql`query {\n    blogPostCollection {\n      items{\n        created\n        tags,\n        title,\n        slug,\n        body {\n          json\n        }\n      }\n    }\n  }\n`;\nexport default function Writing() {\n  const {loading,data,error} = useQuery(getPosts);\n  return (\n    <div className=\"writing-page\">\n      <div className=\"writing-container\">\n      <div className=\"writing-header\"> Posts\n      </div>\n      {loading ? <Loading/> : (\n      <ul className=\"writing-posts-container\">\n        {data.blogPostCollection.items.map((post) => {\n          return (\n            <a className=\"writing-post\" href={`/blog/${post.slug}`}>\n              <p className=\"writing-post-tag\">{post.tags.map((tag,idx) => {\n                if (idx !== post.tags.length - 1) {\n                  return `${tag} â€¢ `\n                } else {\n                  return tag;\n                }\n              })}</p>\n              <h2 className=\"writing-post-title\">{post.title}</h2>\n              <p className=\"writing-post-date\">{post.created.substring(0,10)}</p>\n              <p className=\"writing-post-descrip\">{post.body.json.content[0].content[0].value.substring(0,120)}...</p>\n            </a>\n          )\n        })}\n        \n      </ul>)}\n      </div>\n    </div>\n  )\n}\n","/Users/baileyritchie/Desktop/Projects/portfolio-2021/src/Components/About/About.js",[],"/Users/baileyritchie/Desktop/Projects/portfolio-2021/src/Components/Projects/Projects.js",["86"],"import React from 'react';\nimport './Projects.css';\nimport {gql} from 'apollo-boost';\nimport {useQuery} from '@apollo/react-hooks';\nimport Loading from '../Loading/Loading';\n\nexport default function Projects() {\n  const getProjects = gql`\n